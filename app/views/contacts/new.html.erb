          <div class="gtb-contact-form pt-100 pb-50"> <!-- DELETED CLASS CONTACT-FORM & ADDED GTB-CONTACT-FORM-->
            <div class="row justify-content-center mt-20">
                <div class="col-lg-6">
                    <div class="contact-form form-style-one mt-35 wow fadeIn" data-wow-duration="2s" data-wow-delay="0.4s">
                      <%= simple_form_for @contact do |f| %>
                        <div class="form-group">
                            <%= f.input :name, label: false, placeholder: 'Nombre *', error: 'Porfavor danos tu nombre' %>
                            <%= f.input :email, label: false, placeholder: 'Correo Electronico *',
                                error: 'El correo no es valido',
                                required: true,
                                autofocus: true,
                                input_html: { autocomplete: "email" } %>
                            <%= f.input :message, label: false, placeholder: 'Preguntas o mensaje *', :as => :text, :input_html => { 'rows' => 8, 'cols' => 10 } %>
                            </div>
                            <%= f.button :submit, 'ENVIAR', class: 'send-button'%>
                            <div class= "hidden">
                            <%= f.input :nickname, :hint => 'Leave this field blank!' %>
                            </div>
                       <% end %>
                    </div> <!-- contact form -->
                </div>
            </div> <!-- row -->
        </div> <!-- contact form DELETED-->

